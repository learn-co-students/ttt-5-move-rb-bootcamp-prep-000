#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
user_input = gets.chomp
integerinput = input_to_index(user_input)
move(board, integerinput)
puts display_board(board)
