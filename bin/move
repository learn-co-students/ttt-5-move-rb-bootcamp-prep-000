#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" "," "," "," "," "," "," "," "," "]
display_board(board)
puts "Where would you like to go?"
your_move = gets.strip
index = input_to_index(your_move)
move(board,index)
display_board(board)
