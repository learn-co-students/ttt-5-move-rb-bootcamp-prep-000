#!/usr/bin/env ruby

require_relative '../lib/move.rb'

board = [" "," "," "," ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"
puts "Please put a number 1 -9"
input = gets.strip

index = input_to_index(input)

move(board,index)
    #index(board)

display_board(board)
 



