#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
board = [" "," "," "," "," "," "," "," "," "]
puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
input = gets.strip

input_to_index(input)

move(board, index = 0, player_char = "X")

display_board(board)

puts board
