#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

board = [" "," "," "," "," "," "," "," "," "]

puts "Welcome to Tic Tac Toe!"
puts "Please enter 1-9:"
user_input = gets.strip

index = input_to_index(user_input)

move(board, index, player_character = "X")
display_board(board)



